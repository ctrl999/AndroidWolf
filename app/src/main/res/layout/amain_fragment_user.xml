                    android:background="@drawable/cart_num_bg"
                    android:gravity="center"
                    android:text="12"
                    android:textColor="@color/t_white"
                    android:textSize="10sp" />

            </RelativeLayout>
        </LinearLayout>

        <RelativeLayout
