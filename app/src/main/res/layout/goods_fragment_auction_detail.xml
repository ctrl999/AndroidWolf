<?xml version="1.0" encoding="utf-8"?>
<com.pkqup.commonlibrary.view.goodspage.PageContainer xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_white">

    <com.pkqup.commonlibrary.view.goodspage.Page
        android:id="@+id/pageOne"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="vertical">


    </com.pkqup.commonlibrary.view.goodspage.Page>

    <com.pkqup.commonlibrary.view.goodspage.Page
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/pageBehavior">

        <WebView
            android:id="@+id/webView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </com.pkqup.commonlibrary.view.goodspage.Page>

</com.pkqup.commonlibrary.view.goodspage.PageContainer>
